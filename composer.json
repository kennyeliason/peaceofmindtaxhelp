{
    "name": "roots/bedrock",
    "type": "project",
    "license": "MIT",
    "description": "WordPress boilerplate with modern development tools, easier configuration, and an improved folder structure",
    "homepage": "https://roots.io/bedrock/",
    "authors": [
        {
            "name": "Scott Walkinshaw",
            "email": "scott.walkinshaw@gmail.com",
            "homepage": "https://github.com/swalkinshaw"
        },
        {
            "name": "Ben Word",
            "email": "ben@benword.com",
            "homepage": "https://github.com/retlehs"
        }
    ],
    "keywords": [
        "bedrock",
        "composer",
        "roots",
        "wordpress",
        "wp",
        "wp-config"
    ],
    "support": {
        "issues": "https://github.com/roots/bedrock/issues",
        "forum": "https://discourse.roots.io/category/bedrock"
    },
    "repositories": {
        "neonbrand": {
            "type": "composer",
            "url": "https://mainwp.neonbrand.com/satispress/"
        },
        "0": {
            "type": "composer",
            "url": "https://wpackagist.org",
            "only": [
                "wpackagist-plugin/*",
                "wpackagist-theme/*"
            ]
        }
    },
    "require": {
        "php": ">=7.1",
        "composer/installers": "^1.8",
        "vlucas/phpdotenv": "^5.3",
        "oscarotero/env": "^2.1",
        "roots/wordpress": "^5.8",
        "roots/wp-config": "1.0.0",
        "roots/wp-password-bcrypt": "1.0.0",
        "primetime/wp-plugin-activation-manifest": "^1.0",
        "wpackagist-plugin/post-type-switcher": "^3.2",
        "wpackagist-plugin/mailgun": "^1.7",
        "wpackagist-plugin/better-search-replace": "^1.3",
        "wpackagist-plugin/what-template-file-am-i-viewing": "^1.2",
        "wpackagist-plugin/regenerate-thumbnails": "^3.1",
        "wpackagist-plugin/mainwp-child": "^4.0",
        "wpackagist-plugin/wp-time-capsule": "^1.21",
        "wpackagist-plugin/wp-user-avatars": "^1.4",
        "wpackagist-plugin/wp-user-profiles": "^2.5",
        "roots/soil": "^4.0",
        "doctrine/inflector": "^1.3",
        "neonbrand/gravityformsmailchimp": "^5.0",
        "neonbrand/media-file-renamer-pro": "^5.0",
        "neonbrand/ithemes-security-pro": "^7.0",
        "neonbrand/gravityforms": "^2.4",
        "wpackagist-plugin/gravityformscli": "^1.1",
        "neonbrand/cf-loader": "^1.0",
        "neonbrand/wordpress-seo-premium": "^17.7",
        "neonbrand/google-analytics-premium": "^8.2",
        "neonbrand/monsterinsights-forms": "^2.1",
        "neonbrand/monsterinsights-dimensions": "^2.0",
        "neonbrand/monsterinsights-performance": "^1.6",
        "neonbrand/monsterinsights-page-insights": "^1.4",
        "neonbrand/relevanssi-premium": "^2.1",
        "neonbrand/advanced-custom-fields-pro": "^5.8",
        "neonbrand/wpseo-local": "^14.2",
        "wpackagist-plugin/goodbye-captcha": "^3.1",
        "wpackagist-plugin/akismet": "^4.1",
        "wpackagist-plugin/clickdesk-live-support-chat-plugin": "^4.41",
        "wpackagist-plugin/conditional-widgets": "^3.0",
        "wpackagist-plugin/easyazon": "^5.0",
        "neonbrand/easyzon-tag-replacer": "^0.0.2",
        "neonbrand/easyazon-pro": "^4.0",
        "wpackagist-plugin/feature-a-page-widget": "^2.1",
        "wpackagist-plugin/featured-images-for-rss-feeds": "^1.5",
        "wpackagist-plugin/cookie-law-info": "^2.0",
        "wpackagist-plugin/duracelltomi-google-tag-manager": "^1.11",
        "neonbrand/gravityformszohocrm": "^2.5",
        "wpackagist-plugin/social-networks-auto-poster-facebook-twitter-g": "^4.3",
        "wpackagist-plugin/optinmonster": "^2.2",
        "wpackagist-plugin/recent-posts-widget-extended": "^1.0",
        "wpackagist-plugin/unbounce": "^1.0",
        "wpackagist-plugin/wordpress-popular-posts": "^5.0",
        "neonbrand/wpseo-news": "^13.1",
        "neonbrand/simple-sitemap-pro": "^3.5",
        "wpackagist-plugin/bulk-block-converter": "^1.0",
        "wpackagist-plugin/infusionsoft": "^1.5",
        "neonbrand/gravityformszapier": "^4.1",
        "wpackagist-plugin/duplicate-post": "^4.1",
        "wpackagist-plugin/remove-nofollow": "^1.00",
        "neonbrand/roots-share-buttons": "^1.2",
        "neonbrand/nxs-snap-pro-upgrade": "^1.5",
        "neonbrand/wp-rocket": "^3.7",
        "wpackagist-plugin/imagify": "^1.9",
        "wpackagist-plugin/yoast-test-helper": "^1.11",
        "neonbrand/optinmonster": "^2.0",
        "wpackagist-plugin/cms-tree-page-view": "^1.6",
        "wpackagist-plugin/elementor": "^3.1",
        "neonbrand/elementor-pro": "^3.2",
        "wpackagist-plugin/nitropack": "^1.5",
        "wpackagist-plugin/wordpress-seo": "^17.7",
        "neonbrand/monsterinsights-google-optimize": "^1.3",
        "neonbrand/monsterinsights-ads": "^1.1",
        "neonbrand/rocket-specify-image-dimensions": "^1.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.6.0",
        "roave/security-advisories": "dev-master"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "allow-plugins": {
            "composer/installers": true,
            "roots/wordpress-core-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "installer-paths": {
            "web/app/mu-plugins/{$name}/": [
                "type:wordpress-muplugin"
            ],
            "web/app/plugins/{$name}/": [
                "type:wordpress-plugin"
            ],
            "web/app/themes/{$name}/": [
                "type:wordpress-theme"
            ]
        },
        "wordpress-install-dir": "web/wp"
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "test": [
            "phpcs"
        ]
    }
}
